<h1>編集画面</h1>

<%= form_with model: @list, url:update_list_path(@list.id), method: :patch do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <%= f.submit '保存' %>

<% end %>

<!--3行目のupdate_list_pathのとこ、update消すとエラー解決できる
    最後までやってカリキュラムに記述がなければ質問する-->