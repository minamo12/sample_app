<div class="list-wrap">
<% @lists.each do |list| %>
  <div class="list-box">
    <% if list.image.attached? %>
      <%= image_tag list.image, width: "200px", height: "200px" %>
    <%  else %>
      <%= image_tag 'no_image', width: "200px", height: "200px" %>
    <% end %>
    <div class="list-comment">
      <span class="list-body"><%= list.body %></span>
      <span class="list-title"><%= link_to list.title, list_path(list.id) %></span>
    </div>
  </div>
<% end %>
</div>